#include <stdio.h>
#include <stdlib.h>
 
main(){
    int x, y, z, CONT, soma=0;
    scanf("%d%d", &x,&y);
     
    // Execulta o comando e deixa em ordem crecente
    while((x>0) && (y>0)){
      if (y<x){ 
        z=x;
        x=y;
        y=z;
    }
    // guarda e soma os valores
    for(CONT=x; CONT<=y; CONT++){
        printf("%d ", CONT);
        soma+=CONT;
    }
    // Vai mostrar o meu resultado    
    printf("Sum=%d\n", soma);
    soma=0;
     
    // Guarda valores para o laÃ§o while
    scanf("%d%d", &x,&y); 
    }
    return 0;
}
