#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main()
{
    char z[1110];
    int y,somar=0,a,b;
    scanf("%d", &y);
        for(a=0 ; a < y ; a++){
            scanf("%s", z);
             
        // strlen para contar o meu numeros de caracteres na minha string    
                for( b=0 ; b < strlen(z) ; b++){
        if(z[b] == '1'){
            somar+=2;
        }
        else if(z[b] == '2'){
            somar+=5;
        }
        else if(z[b] == '3'){
            somar+=5;
        }
        else if(z[b] == '4'){
            somar+=4;
        }
        else if(z[b] == '5'){
            somar+=5;
        }
        else if(z[b] == '6'){
            somar+=6;
        }
        else if(z[b]== '7'){
            somar+=3;
        }
        else if(z[b] == '8'){
            somar+=7;
        }
        else if(z[b] == '9'){
            somar+=6;
        }
        else if(z[b] == '0'){
            somar+=6;
        }
                }
        printf("%d leds\n",somar);
            somar = 0;
        }
  
    return 0;
}
